    "def createDummies(df, var_name):\n",
    "    import pandas as pd\n",
    "    dummy = pd.get_dummies(df[var_name], prefix=var_name)\n",
    "    df = df.drop([var_name], axis=1)\n",
    "    df = pd.concat([df, dummy], axis=1)\n",
    "    return df"
   ]
